{
  "name": "VR-planetary",
  "version": "1.0.0",
  "description": "Сборка для проекта VR Planetary",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "eslint": "6.8.0",
    "eslint-config-htmlacademy": "0.4.x",
    "gulp": "4.0.x",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-terser": "^1.2.0",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.2.0",
    "stylelint": "9.10.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "terser-webpack-plugin": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "eslint": "eslint \"source/js/**/*.js\"",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/modules/**/*.js",
    "source/img/**/*.svg",
    "source/sass/*.{sass,scss}",
    "source/sass/blocks/**/*.{sass,scss}"
  ],
  "eslintIgnore": [
    "vendor/"
  ],
  "engines": {
    "node": "8.15"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "del": "^4.1.1",
    "imask": "^5.2.1",
    "slick-carousel": "^1.8.1",
    "swiper": "^5.0.2"
  }
}
